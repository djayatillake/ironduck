# Array indexing tests

# Basic array indexing (1-based)
query I
SELECT [1, 2, 3][1];
----
1

query I
SELECT [1, 2, 3][2];
----
2

query I
SELECT [1, 2, 3][3];
----
3

# Out of bounds returns NULL
query T
SELECT [1, 2, 3][0];
----
NULL

query T
SELECT [1, 2, 3][4];
----
NULL

# Array slice
query T
SELECT ARRAY_SLICE([1, 2, 3, 4, 5], 2, 4);
----
[2, 3, 4]

query T
SELECT ARRAY_SLICE([1, 2, 3, 4, 5], 1, 3);
----
[1, 2, 3]

# UNNEST tests
query I rowsort
SELECT * FROM UNNEST([1, 2, 3]);
----
1
2
3

query I rowsort
SELECT x FROM UNNEST([10, 20, 30]) AS t(x);
----
10
20
30

